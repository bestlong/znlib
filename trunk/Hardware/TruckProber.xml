<?xml version="1.0" encoding="gb2312"?>
<!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
节点: prober
*.ip,port: 控制器地址
*.enable: 1为启用

节点: prober->tunnels->tunnel
*.in: 输入通道编号(1-8)
*.out: 输出通道编号(1-8)
*.enable: 0为关闭
+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
<probers>
  <prober id="N1" name="一期1号磅">
    <ip>192.168.200.45</ip>
    <port>6000</port>
    <enable>1</enable>
    <tunnels>
      <tunnel id="T1" name="1号进">
        <in>1</in>
        <out>1</out>
      </tunnel>
    </tunnels>
  </prober>
</probers>
